<% if @success %>
$('#div_dashboard_show').html("<%= escape_javascript(render(:partial=>'/admin/dashboards/show')) %>");
$('#div_wallboard_button').html("<%= escape_javascript(render(:partial=>'/admin/dashboards/wallboard_button')) %>");
$('.modal-backdrop').hide();
<% else %>
heading = "Add Widgets";
bodyContent = "<%= escape_javascript(render(:partial=>'form')) %>";
showModal(heading, bodyContent);
<% end %>